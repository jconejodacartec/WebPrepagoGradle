<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/java2ee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<application>
		<view-handler>com.ibm.faces20.portlet.FaceletPortletViewHandler</view-handler>
		<el-resolver>com.ibm.faces20.portlet.PortletELResolver</el-resolver>
		<resource-handler>com.ibm.faces20.portlet.httpbridge.PortletResourceHandler</resource-handler>
	</application>
	<component>
		<component-type>com.ibm.faces20.portlet.component.PortletActionURL</component-type>
		<component-class>com.ibm.faces20.portlet.component.PortletActionURL</component-class>
	</component>
	<component>
		<component-type>com.ibm.faces20.portlet.component.PortletResourceURL</component-type>
		<component-class>com.ibm.faces20.portlet.component.PortletResourceURL</component-class>
	</component>
	<component>
		<component-type>com.ibm.faces20.portlet.component.PortletRenderURL</component-type>
		<component-class>com.ibm.faces20.portlet.component.PortletRenderURL</component-class>
	</component>
	<component>
		<component-type>com.ibm.faces20.portlet.component.PortletParam</component-type>
		<component-class>com.ibm.faces20.portlet.component.PortletParam</component-class>
	</component>
	<component>
		<component-type>com.ibm.faces20.portlet.component.PortletProperty</component-type>
		<component-class>com.ibm.faces20.portlet.component.PortletProperty</component-class>
	</component>
	<component>
		<component-type>com.ibm.faces20.portlet.component.PortletNameSpace</component-type>
		<component-class>com.ibm.faces20.portlet.component.PortletNameSpace</component-class>
	</component>
		<managed-bean>
		<managed-bean-name>pc_RegistrarUsuarioBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.RegistrarUsuarioBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_AdministrarTarjetaBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.AdministrarTarjetaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_ConsultarMovimientosBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.ConsultarMovimientosBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_ConsultarSaldoBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.ConsultarSaldoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>pc_EliminarUsuariosInactivosBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.EliminarUsuariosInactivosBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_AsignarContrasenaBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.AsignarContrasenaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_AdministrarParametrosWPView</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.AdministrarParametrosWPBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_MensajeBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.MensajeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_ModificarInfoPersonalBean</managed-bean-name>
		<managed-bean-class>com.rbm.web.prepago.managedBeans.ModificarInfoPersonalBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<render-kit>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>com.ibm.faces20.portlet.tag.render.ActionURLTagRender</renderer-type>
			<renderer-class>com.ibm.faces20.portlet.tag.render.ActionURLTagRender</renderer-class>
		</renderer>
	</render-kit>
	<render-kit>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>com.ibm.faces20.portlet.tag.render.ResourceURLTagRender</renderer-type>
			<renderer-class>com.ibm.faces20.portlet.tag.render.ResourceURLTagRender</renderer-class>
		</renderer>
	</render-kit>
	<render-kit>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>com.ibm.faces20.portlet.tag.render.RenderURLTagRender</renderer-type>
			<renderer-class>com.ibm.faces20.portlet.tag.render.RenderURLTagRender</renderer-class>
		</renderer>
	</render-kit>
	<render-kit>
		<renderer>
			<component-family>javax.faces.Output</component-family>
			<renderer-type>com.ibm.faces20.portlet.tag.render.PortletNameSpaceTagRender</renderer-type>
			<renderer-class>com.ibm.faces20.portlet.tag.render.PortletNameSpaceTagRender</renderer-class>
		</renderer>
	</render-kit>
    
</faces-config>